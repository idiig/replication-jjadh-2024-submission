@article{Vrbinc2015Diasystematic,
  title = {Diasystematic {{Information}} in {{Learner}}'s {{Dictionaries}}: {{The Usability}} of {{Multiple Labels}}},
  shorttitle = {Diasystematic {{Information}} in {{Learner}}'s {{Dictionaries}}},
  author = {Vrbinc, Marjeta and Vrbinc, Alenka},
  year = {2015},
